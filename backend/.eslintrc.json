{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base",
    "plugin:prettier/recommended"
  ],
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    //    "react/function-component-definition": [
    //      2,
    //      {
    //        "namedComponents": [
    //          "function-declaration",
    //          "arrow-function"
    //        ]
    //      }
    //    ],
    "import/prefer-default-export": "off",
    "no-alert": "off",
    "no-console": "off",
    "import/extensions": "off",
    "no-underscore-dangle": "off",
    "consistent-return": "off"
  }
}
